{
  "version": 4,
    "include": [
    "base.json"
  ],
  "configurePresets": [
    {
      "name": "macos.arm64",
      "inherits": "base",
      "toolchainFile": "$env{ARIEO_BUILDENV_PACKAGE_INSTALL_FOLDER}/conan/host/macos.arm64/conan_toolchain.cmake",
      "architecture": 
      {
        "value": "arm64",
        "strategy": "external"
      },
      "environment": {
        "CLANG_FOR_CODEGEN": "$env{DEVCRAFT_WORKSPACE}/llvm/llvm-21/$env{DEVCRAFT_HOST_TRIPLE}/bin/clang++"
      },
      "cacheVariables": {
        "ARIEO_HOST_OS": "macos",
        "TARGET_TRIPLE": "arm64-apple-darwin",
        "CMAKE_PREFIX_PATH": {
          "type": "PATH",
          "value": "$env{ARIEO_THIRDPARTIES_PACKAGE_INSTALL_FOLDER}/conan/macos.arm64"
        }
      }
    }
  ]
}
